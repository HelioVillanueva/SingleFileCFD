FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "3";
    object      gradTz;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 -1 0 1 0 0 0];

internalField   uniform 0;

boundaryField
{
    left
    {
        type            calculated;
        value           uniform 0;
    }
    Out
    {
        type            calculated;
        value           uniform 0;
    }
    topAndBottom
    {
        type            empty;
    }
    frontAndBack
    {
        type            empty;
    }
}


